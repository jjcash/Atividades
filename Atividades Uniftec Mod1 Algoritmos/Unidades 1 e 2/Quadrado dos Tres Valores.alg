algoritmo "quadrado dos tres valores"
var
 vlr1, vlr2, vlr3, resultado : real
inicio
 escreva("Insira o Primeiro Valor: ")
 leia(vlr1)
 escreva("Insira o Segundo Valor: ")
 leia(vlr2)
 escreva("Insira o Terceiro Valor: ")
 leia(vlr3)
 resultado <- (vlr1+vlr2+vlr3)^2
 escreva("Resultado: ", resultado)
fimalgoritmo
